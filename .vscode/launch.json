{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build: FsDnsProxy.fsproj",
            "program": "${workspaceFolder}/FsDnsProxy/bin/Debug/net10.0/FsDnsProxy.dll",
            "args": ["settings.yaml"],
            "cwd": "${workspaceFolder}/FsDnsProxy",
            "stopAtEntry": false,
            "console": "internalConsole",
        },
        {
            "name": "Docker .NET Container SDK Launch",
            "type": "docker",
            "request": "launch",
            "netCore": {
                "appProject": "${workspaceFolder}/FsDnsProxy/FsDnsProxy.fsproj",
                "buildWithSdk": true
            }
        }
    ]
}